{
  "name": "@extension/e2e",
  "version": "0.5.0",
  "description": "E2e tests configuration boilerplate",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "e2e": "wdio run config/wdio.browser.conf.ts",
    "clean:node_modules": "yarn dlx rimraf node_modules",
    "clean:turbo": "yarn dlx rimraf .turbo",
    "clean": "yarn clean:turbo && yarn clean:node_modules"
  },
  "devDependencies": {
    "@extension/env": "workspace:*",
    "@extension/tsconfig": "workspace:*",
    "@wdio/cli": "^9.14.0",
    "@wdio/globals": "^9.14.0",
    "@wdio/local-runner": "^9.14.0",
    "@wdio/mocha-framework": "^9.14.0",
    "@wdio/spec-reporter": "^9.14.0",
    "@wdio/types": "^9.14.0"
  }
}
